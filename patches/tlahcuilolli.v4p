  <NODE id="12" nodename="Cons (String)" systemname="Cons (String)" componentmode="Hidden">
  <BOUNDS type="Node" left="1470" top="5880" width="100" height="100">
  </BOUNDS>
  <PIN pinname="Input 1" visible="1" values="|Axcan tica ipan inic ce centlaxitl, in ohtli campa in tlalli centilia ihuan mictlan... Motlamiliztli tiyahui nahui xihuitl ixquixca in zatlatzonco centlalixtli, Itzmictlan Apochcalocan ihuan tihuica ce tlalmanalli inic Mictlantecuhtli.|">
  </PIN>
  <PIN pinname="Input 2" visible="1" values="|In atl ce tenolli nepantla inin ome tlalticpac, in miquiztli panozqueh ce atoyatl acopan ce itzcuintli cuitlaxiloyotl, inin mocuepaz iteyacaniliztli ipan nochi ohtli...|">
  </PIN>
  <PIN pinname="Input 3" visible="1" values="|Mah Xitihuiquiliaz tipanoz nepantla tepemeh, intla anmopachohuazqueh anmopopoloazqueh tlacahualtica moaci ompa Mictlan. ¡Cuetlahuiliztli! Ihcuac in Oactli tototl cuica, mocaquiztilia in tlapopoliztli...|">
  </PIN>
  <PIN pinname="Input 4" visible="1" values="|Mah ximopanoltitzino ompa tlacuatlalli, centlaliztli ipan Tepetl  Monamiquian ihuan Itzehecayan, ce ohtli cenca ohui ompa tihuelitiz nextiaz in tlapopoliztli intla ahmo netlacuitlahuini|">
  </PIN>
  <PIN pinname="Input 5" visible="1" values="|In achi cehuatic ohtli ihuicpa in nehnemiliztli, achi chicahuac ehecaohtli. In tetl, cuauhmeh ihuan itztlaxotlahuiqui mehuah, mah ximocuitlahuia tlein amoh yahuih ompa tehuatl|">
  </PIN>
  <PIN pinname="Input 6" visible="1" values="|Ompa Pantiyan, in miquiztli ixittah ihuan quixmatih itonalli, Cuix tiaci ompa tlamiliztlic centlaxitl ihuan  ticmaca Mictlantecuhtli motlalmanalli,  nozzo, cuix in ohtli timopopoloaz...|">
  </PIN>
  <PIN pinname="Input 7" visible="1" values="||">
  </PIN>
  <PIN pinname="Input 8" visible="1" values="||">
  </PIN>
  <PIN pinname="Input 9" visible="1" values="||">
  </PIN>
  <PIN pinname="Input Count" visible="0" values="|9|">
  </PIN>
  <PIN pinname="Descriptive Name" visible="0" values="||">
  </PIN>
